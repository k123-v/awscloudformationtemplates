{
    "AWSTemplateFormatVersion" : "2010-09-09",
    "Description" : "My First Ec2 Template using cloud Formation Template.",
    "Metadata" : {
        "Instances" : {"Description": "Here is my information on Ec2"}

    },
    "Parameters" : {
        "InstanceParameterTypes" : {
            "Type" : "String",
            "Default" : "t2.micro",
            "AllowedValues": ["t2.micro","m1.small","t2.small"],
            "Description" : "Enter t2.micro,m1.small,or t2.small . Default is t2.micro."
        },
        "KeyName" : {
            "Description" : "Name of an Keypair to Enable ssh access",
            "Type":"AWS::EC2::KeyPair::KeyName",
            "Default" :"feb20key",
            "ConstraintDescription" : "must be the name of an exisitng key pair."
        }     
    },
    "Resources" : {
        "EC2Instance" : {
            "Type" : "AWS::EC2::Instance",
            "Properties" : {
                "InstanceType" : "t2.micro",
                "AvailabilityZone" : "us-east-1a",
                "ImageId": "ami-0915bcb5fa77e4892",
                "KeyName" : {"Ref" : "KeyName" },
                "SubnetId" : "subnet-ea4eddcb",
                "Tags" : [
                    {
                        "Key": "NAME",
                        "Value" : "KowshikcfEc2"
                    }
                ]
                
                
            }
        }
    },
    "Outputs" : {
        "InstanceId" : {
            "Description" :"InstanceId of Newly created Ec2 Through cloud Formation Template",
            "Value" : {"Ref":"EC2Instance"}
        },
        "AZ" : {
            "Description" : "Avaliblity ZOne of newly created Ec2 instance" ,
            "Value" : {"Fn::GetAtt" :["EC2Instance","AvailabilityZone"]}
        }

    }

}